{
  "name": "backend-starter",
  "version": "0.2.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && pnpm run copy:assets",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "pnpm run copy:assets && nest start --watch",
    "start:debug": "pnpm run copy:assets && nest start --debug 0.0.0.0:9229 --watch --preserveWatchOutput",
    "start:prod": "pnpm run copy:assets && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --forceExit --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage && jest-badges-readme --coverageDir='./coverage'",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "release": "standard-version",
    "lint-staged": "lint-staged",
    "make-badges": "jest-badges-readme --coverageDir='./coverage'",
    "clean": "rimraf ./node_modules && rimraf ./yarn.lock && rimraf ./package-lock.json",
    "copy:assets": "cpx './config/**' './dist/config'",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migration:generate": "sequelize-cli migration:generate",
    "seed": "sequelize-cli db:seed",
    "seed:all": "sequelize-cli db:seed:all",
    "seed:undo:all": "sequelize-cli db:seed:undo:all",
    "seed:generate": "sequelize-cli seed:generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/register": "^7.21.0",
    "@faker-js/faker": "^7.6.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/sequelize": "^9.0.2",
    "@nestjs/swagger": "^6.3.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "helmet": "^6.1.5",
    "nest-keycloak-connect": "^1.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "qs": "^6.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "sequelize": "^6.31.0",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^4.6.2",
    "undici": "^5.22.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@olavoparno/jest-badges-readme": "^1.5.1",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/faker": "^6.6.8",
    "@types/jest": "29.5.1",
    "@types/node": "^18.16.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/qs": "^6.9.7",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "cpx": "^1.5.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "lint-staged": ">=13.2.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
