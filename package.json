{
  "name": "backend-starter",
  "version": "0.2.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "rimraf ./dist && rimraf ./src/metadata.ts && nest build && pnpm run copy:assets",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "pnpm run copy:assets && nest start --watch",
    "start:debug": "pnpm run copy:assets && nest start --debug 0.0.0.0:9229 --watch --preserveWatchOutput",
    "start:prod": "pnpm run copy:assets && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --forceExit --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage && jest-badges-readme --coverageDir='./coverage'",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "release": "standard-version",
    "lint-staged": "lint-staged",
    "make-badges": "jest-badges-readme --coverageDir='./coverage'",
    "clean": "rimraf ./node_modules && rimraf ./yarn.lock && rimraf ./package-lock.json",
    "copy:assets": "cpx './config/**' './dist/config'",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migration:generate": "sequelize-cli migration:generate",
    "seed": "sequelize-cli db:seed",
    "seed:all": "sequelize-cli db:seed:all",
    "seed:undo:all": "sequelize-cli db:seed:undo:all",
    "seed:generate": "sequelize-cli seed:generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "@babel/register": "^7.22.5",
    "@faker-js/faker": "^8.0.2",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/common": "^10.0.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.3",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.3",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/swagger": "^7.0.4",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "graphql": "^16.7.1",
    "helmet": "^7.0.0",
    "nest-keycloak-connect": "^1.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "qs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^4.6.3",
    "undici": "^5.26.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@nestjs/cli": "^10.0.3",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@olavoparno/jest-badges-readme": "^1.5.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.66",
    "@swc/jest": "^0.2.26",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/faker": "^6.6.8",
    "@types/jest": "29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/qs": "^6.9.7",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "cpx": "^1.5.0",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "lint-staged": ">=13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
